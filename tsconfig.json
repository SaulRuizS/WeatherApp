{
    "compilerOptions": {
        "outDir": "./public/dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "ES6",
        "target": "ES5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "Node",
        "strict": true
    },
    "exclude": [
        "node_modules"
    ]
}